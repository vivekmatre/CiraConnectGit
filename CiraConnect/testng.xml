<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<!--DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd"-->
<suite name="CiraConnect">
  <!--test thread-count="1" name="CiraConnect Login Test Cases" enabled ="true">
    <classes>
      <class name="ciraLogin.Login">
      </class>
    </classes>
  </test--> 
  
  
   <test thread-count="1" name="Login Test with Valid Inputs" enabled ="true">
    <classes>
      <class name="ciraLogin.Login">
      	<methods>
      		<include name="ValidInputLoginTest"></include>
      	</methods>
      </class>
    </classes>
  </test> 
  
    <test thread-count="1" name="Login Test with InValid Inputs" enabled ="true">
    <classes>
      <class name="ciraLogin.Login">
      	<methods>
      		<include name="InValidInputLoginTest"></include>
      	</methods>
      </class>
    </classes>
  </test>
  
    <test thread-count="1" name="Show Password Functionality" enabled ="true">
    <classes>
      <class name="ciraLogin.Login">
      	<methods>
      		<include name="ShowPasswordFunctionality"></include>      		
      	</methods>
      </class>
    </classes>
  </test> 
  
   <test thread-count="1" name="Verify Login with Blank Input Fields" enabled ="true">
    <classes>
      <class name="ciraLogin.Login">
      	<methods>
      		<include name="LoginWithEmptyField"></include>      		
      	</methods>
      </class>
    </classes>
  </test> 
   
  
</suite> 